import serverApi from "@/api/server.api";

const getDialogs = () => {
    return serverApi.get('dialogs/')
}

const getMessages = (listParams) => {
    return serverApi.get('messages/', {params: listParams})
}

const newMessage = (data) => {
    return serverApi.post('messages/new', data)
}

const searchUser = (listParams) => {
    return serverApi.get('search-user/', {params: listParams})
}

export default {getDialogs, getMessages, newMessage, searchUser}
